// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+alt+v",
        "command": "telesoho.MarkdownPaste",
        "when": "editorTextFocus && resourceLangId == 'markdown'"
    },
    {
        "key": "ctrl+alt+v",
        "command": "-telesoho.MarkdownPaste",
        "when": "editorTextFocus && resourceLangId == 'markdown'"
    },
    {
        "key": "alt+w",
        "command": "workbench.action.closeActiveEditor"
    }
]